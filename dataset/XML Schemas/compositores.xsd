<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:element name="compositores">
        <xs:complexType>
            <xs:sequence maxOccurs="unbounded">
                <xs:element name="compositor">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="nome" type="xs:string"/>
                            <xs:element name="bio" type="xs:string"/>
                            <xs:element name="dataNasc" type="xs:string"/>
                            <xs:element name="dataObito" type="xs:string"/>
                            <xs:element name="periodo" type="xs:string"/>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:ID" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
